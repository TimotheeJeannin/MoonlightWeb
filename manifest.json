{
  "manifest_version": 2,
  "name": "MoonlightWeb Extension",
  "version": "1.0.0",
  "permissions": [
    "storage",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "node_modules/jquery/dist/jquery.min.js",
        "node_modules/jquery-color/jquery.color.js",
        "main.js"
      ]
    }
  ]
}
